{
  "name": "500cats",
  "version": "1.0.0",
  "scripts": {
    "bootstrap": ". local-env.sh && tsc && node server/bootstrap",
    "dev": ". local-env.sh && tsc && concurrently \"tsc --watch\" \"nodemon --watch 'server/**/*.js' --exec 'node server -p $PORT'\"",
    "start": "NODE_ENV=production node server",
    "build": "tsc && next build",
    "test": "tsc && jest",
    "test:watch": "tsc && jest --watch"
  },
  "dependencies": {
    "aws-sdk": "^2.122.0",
    "axios": "^0.16.2",
    "jest": "^21.1.0",
    "multer": "^1.3.0",
    "next": "latest",
    "pg": "^7.3.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "sequelize": "^4.10.2"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/express": "^4.0.37",
    "@types/jest": "^21.1.0",
    "@types/multer": "^1.3.3",
    "@types/next": "^2.4.2",
    "@types/node": "^8.0.30",
    "@types/pg": "^7.1.2",
    "@types/react": "^15.0.1",
    "@types/sequelize": "^4.0.74",
    "@types/supertest": "^2.0.3",
    "concurrently": "^3.1.0",
    "express": "^4.15.4",
    "nodemon": "^1.12.1",
    "supertest": "^3.0.0",
    "typescript": "^2.1.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx)"
    ]
  }
}
